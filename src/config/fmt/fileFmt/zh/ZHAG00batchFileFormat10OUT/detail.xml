<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
    encoding="gbk" trace="true">
	<segment name="detail" encoding="GBK">
     <fixString name="rsvFld1"       length="6" />     <!-- 来盘序号 -->
      <fixString name="cusAc"   length="30" />    <!-- 帐号 -->
      <fixString name="txnAmt"   length="15"  expression='exp:str.leftPad(num.centToYuanAsString(this.txnAmt),15," ")'/>   <!-- 来盘金额元为单位，含小数点 -->
      <fixString name="thdCusNme"  length="30" expression='exp:str.leftPad(this.thdCusNme,30," ")' />    
      <fixString name="rsvFld2"   		length="12" />    <!-- 保单费用代码 -->
      <fixString name="SucFlg"   		length="1" />  
	</segment>
</message>

