<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
    encoding="gbk" trace="true">
	<segment name="detail" encoding="GBK">
	<fixString name="thdCusNo"   length="20" expression='exp:str.leftPad(this.thdCusNo,20," ")'/>    <!-- 电话号码 -->
      <fixString name="thdCusNme"   length="40" expression='exp:str.leftPad(this.thdCusNme,40," ")'/>    <!-- 客户名称 -->
      <fixString name="cusAc"   length="30" expression='exp:str.leftPad(this.cusAc,30," ")'/>    <!-- 帐号 -->
      <fixString name="txnAmt"   length="12" expression='exp:str.leftPad(num.centToYuanAsString(this.txnAmt),12," ")'/>   <!-- 来盘金额元为单位，含小数点 -->
	<fixString name="SucFlg"   length="1"  />    <!-- 标识,1-成功,2-失败 -->	

	</segment>
</message>

