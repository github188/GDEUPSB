<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
    encoding="gbk" trace="true">
	<segment name="detail" encoding="GBK">
       <fixString name="sqn"   length="16" />    <!-- 银行流水号 -->
      <fixString name="txnTlr"    length="5"  />    <!-- 操作员号 -->
      <fixString name="txnDte"   length="8"  />    <!-- 银行入帐日期 -->
      <fixString name="TrTime"   length="6"  />    <!-- 收费时间 -->
      <fixString name="rsvFld1"       length="8"  />    <!-- 供电公司编号 -->
      <fixString name="thdCusNo"   length="15" />    <!-- 户号 -->
      <fixString name="rsvFld2"       length="6"  />    <!-- 电费月份 -->
      <fixString name="rsvFld3"       length="2"  />    <!-- 电费次数 -->
      <fixString name="txnAmt"   length="12" />    <!-- 欠费总额，金额元为单位，含小数点，右补空格 -->
      <fixString name="cusAc"   length="32" />    <!-- 客户帐号 -->
      <fixString name="thdCusNme"   length="32" />    <!-- 客户帐户名称 -->
      <fixString name="rsvFld5"       length="33" />    <!-- 供电公司在银行开户帐号 -->
      <fixString name="rsvFld6"       length="5" />     <!-- 供电公司计量点号 -->
      <fixString name="rsvFld7"       length="16" />    <!-- 供电公司应收凭证号 -->
      
			
			

	</segment>
</message>

