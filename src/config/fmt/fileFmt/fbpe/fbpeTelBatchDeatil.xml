<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
    encoding="utf-8" trace="true">
    <segment name="detail" encoding="GBK">
        <fixString name="sqn" length="20" />  <!-- 交易流水号-->
        <fixString name="cusNo" length="20" />  <!-- 客户编号-->
        <fixString name="rsvFld1" length="16" />  <!-- 备用字段1 代表号码,同一交费编号中的某一号码 -->
        <fixString name="cusNam" length="20" />  <!-- 客户姓名-->
        <fixString name="bankNo" length="5" />  <!-- 备用字段2 代表银行代码 -->
        <fixString name="bankNam" length="30" />  <!-- 备用字段3 代表银行名称 -->
        <fixString name="actNo" length="30"  referred="true" expression="exp:@com.bocom.bbip.gdeupsb.utils.GdExpCommonUtils@subStr(this.txnAMt,0,21)"/>  <!--银行账号-->
        <fixString name="txnAmt" length="10" referred="true" expression="exp:@com.bocom.bbip.gdeupsb.utils.GdExpCommonUtils@transYuanToFen(this.txnAMt)"/>  <!-- 应缴金额(带小数点)-->
        <Item name="months" length="6"/>  <!--话费月份-->

   </segment>
</message>