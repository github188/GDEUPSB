<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
    encoding="utf-8" trace="true">
    <segment name="detail" encoding="GBK">
        <fixString name="length1" length="3" /> 
        <fixString name="cusNo" length="$length1" />  <!-- DVB客户编号-->
        <fixString name="length2" length="3" /> 
        <fixString name="actNo" length="$length2" />  <!--银行账号-->
        <fixString name="length3" length="3" /> 
        <fixString name="cusNam" length="$length3" />   <!-- DVB客户姓名-->
        <fixString name="length4" length="3" />  
        <fixString name="txnAmt" length="$length4" />   <!-- 应缴金额(带小数点)-->

    </segment>
</message>