<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
	encoding="GBK"  trace="true">
	
	<segment name="header" encoding="GBK">
		    <fixString name="PID"     length="20" alias="rsvFld1"/>  <!--批量代收信息标识-->
			<fixString name="CUN"     length="3" alias="rsvFld2"/>   <!--货币符号-->
			<fixString name="FPBB"    length="1" alias="rsvFld3"/>   <!--收付标志,固定为收1-->
			<fixString name="EDD"     length="3" alias="rsvFld4"/>   <!--费项代码-->
			<fixString name="TotCnt"  length="10" alias="totCnt"/>  <!--总记录数-->
			<fixString name="TotAmt"  length="14" alias="totAmt"/>  <!--总金额-->
			<fixString name="TotCnt"  length="10" alias="sucTotCnt"/>  <!--成功总记录数-->
			<fixString name="TotAmt"  length="14" alias="sucTotAmt"/>  <!--成功总金额-->
			<fixString name="TotCnt"  length="10" alias="falTotCnt"/>  <!--失败总记录数-->
			<fixString name="TotAmt"  length="14" alias="falTotAmt"/>  <!--失败总金额-->
			<fixString name="TxnDat"  length="8" alias="exeDte"/>   <!--应划款日期-->
			<fixString name="retCode" length="4" default="0000"/>
			<fixString name="retMsg"  length="128" default="交易成功"/>
			<fixString name="BankCd"  length="12"  alias="rsvFld5"/>   <!--银行代码-->
			<fixString name="WYSB"    length="30" default=" "/>  <!--唯一识别码-->
	</segment>
	
</message>