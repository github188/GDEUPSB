<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
	encoding="GBK" trace="true">
	
	<segment name="detail"  encoding="GBK">
			
			<fixString name="number" length="10" /><!--  序号	XH	Varchar2(10)-->
			<fixString name="thdCusNo" length="20" /><!--  缴费号	JFH	Varchar2(20)-->
			<fixString name="thdCusNme" length="64" /><!--  结算户名称	JSHMC	Varchar2(64)-->
			
			<fixString name="bankNo" length="4" /><!--  扣款银行代码-->
			
			<fixString name="cusAc" length="32" /><!--  扣款账户	KKZH	Varchar2(32)-->
			<fixString name="cusNme" length="128" /><!--  扣款账户名称	KKZHMC	Varchar2(128)-->			
			<fixString name="fulDedFlg" length="1" /><!--  部分缴费标志	BFJFBZ	Varchar2(1)-->
			<fixString name="accountNo" length="16" /><!--   账务流水号	ZWLSH	Varchar2(16)-->
			<fixString name="electricityYearMonth" length="6" /><!--  电费年月	DFNY	Varchar2(6)-->
			<fixString name="paymentMoney" length="16" /><!--  应扣金额	YKJE	Number(14,2)-->
			<fixString name="capital" length="16" /><!--  本金	BJ	Number(14,2)-->
			<fixString name="dedit" length="16" /><!--  违约金	WYJ	Number(14,2)-->
			<fixString name="paymentResult" length="2" /><!--  扣款结果代码	KKJGDM	Varchar2 (2)	2-->
			<fixString name="rsvFld5" length="16" /><!--  实扣金额	SKJE	Number(14,2)	16-->
			<fixString name="bankSqn" length="27" /><!-- 银行收费流水号	YHSFLSH	Varchar2(20)	20-->
			<fixString name="rsvFld1" length="8" /><!-- 扣款日期	KKRQ	Varchar2(8)	8-->
			<fixString name="rsvFld2" length="6" /><!--  扣款时间	KKSJ	Varchar2(6)	6-->
			<fixString name="bakFld" length="128" /><!-- 备注	BZ	Varchar2(128)	128-->
	</segment>
	
</message>