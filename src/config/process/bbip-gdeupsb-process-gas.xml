	<!-- 福彩对账文件下载 -->
	<process id="gdeupsb.downloadLotChkFilAction" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
			<!-- 进入福彩对账文件下载交易，准备数据 -->
			<action id="act" ref="downloadLotChkFileAction" />
			<!-- 发送文件下载地址查询报文，获取文件地址 -->
			<action id="act" ref="qryFilDownloadAdrAction" />
			<!-- 发送文件下载报文，下载文件 -->
			<action id="act" ref="downloadLotFilesAction" />
			<!-- 文件入库 -->
			<action id="act" ref="lotFilImportAction" />
			<!-- 入库后返回前端信息 -->
			<action id="act" ref="aftDownloadLotFilesAction" />
		</actions>
	</process>

	<!-- 福彩返奖（中奖）文件下载 -->
	<process id="gdeupsb.downloadLotPrzFilAction" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
			<!-- 进入福彩对账文件下载交易，准备数据 -->
			<action id="act" ref="downloadLotPrzFileAction" />
			<!-- 发送文件下载地址查询报文，获取文件地址 -->
			<action id="act" ref="qryFilDownloadAdrAction" />
			<!-- 发送文件下载报文，下载文件 -->
			<action id="act" ref="downloadLotFilesAction" />
			<!-- 文件入库 -->
			<action id="act" ref="lotFilImportAction" />
			<!-- 入库后返回前端信息 -->
			<action id="act" ref="aftDownloadLotFilesAction" />
		</actions>
	</process>


