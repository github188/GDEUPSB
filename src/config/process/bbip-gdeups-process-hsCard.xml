<x:beans xmlns:x="http://www.springframework.org/schema/beans"
	xmlns="http://www.bocom.com/schema/jump-core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.bocom.com/schema/jump-core http://www.bocom.com/schema/jump-core-1.0.xsd">
	<!-- 一卡通签到  -->
	<process id="gdeupsb.hSCardSignIn" template="eups.simpleTran" interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.HScardSignInAction"/>
		    <action id="act" ref="eups.eupsManageThdTranCtlDetailAction" />
		</actions>
	</process> 
	
	<!-- 一卡通签退  -->
	<process id="gdeupsb.hSCardSignOut" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.HScardSignOutAction"/>
		    <action id="act" ref="eups.eupsManageThdTranCtlDetailAction" />
		</actions>
	</process> 
	
	<!-- 余额查询  -->
	 <process id="gdeupsb.hsCardFeeQuery" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
		    <!-- <action id="act" class="com.bocom.bbip.gdeupsb.action.common.CheckComTxnCtlAction" operations="checkThdTxnCtlNormal" /> -->
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.HSCardFeeQueryAction"/>
		</actions>
	</process> 
	
	<!-- 圈存对账  -->
	 <process id="gdeupsb.hsCardCheckFile" template="eups.simpleTran"
		interceptors="bbip.bbipSchedulerInterceptor">
		<actions>
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.HSCardCheckFileAction"/>
		</actions>
	</process> 
	
	<!-- 学费查询  -->
	<process id="gdeupsb.stuFeeQry" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.StuFeeQryAction"/>
		</actions>
	</process> 
	
	<!-- 学费缴费  -->
	<process id="gdeupsb.stuPayFee" template="eups.simpleTran"
		interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" class="com.bocom.bbip.gdeupsb.action.hscard.StuPayFeeAction"/>
		</actions>
	</process> 

</x:beans>
