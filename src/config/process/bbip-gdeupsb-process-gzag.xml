<x:beans xmlns:x="http://www.springframework.org/schema/beans"
	xmlns="http://www.bocom.com/schema/jump-core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.bocom.com/schema/jump-core http://www.bocom.com/schema/jump-core-1.0.xsd">
	

	<!-- 根据单位编号查询单位信息 -->
	<process id="gdeupsb.QryComInformation" template="eups.simpleTran"   interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" ref="gdeupsb.QryComInformationAction"/>
		</actions>
	</process> 
	
	<!-- 根据批次号查询信息 
	<process id="ggdeupsb.qryBatInformation" template="eups.simpleTran"   interceptors="eups.transMessageInterceptor">
		<actions>
		    <action id="act" ref="gdeupsb.qryBatInformationAction"/>
		</actions>
	</process> 
	-->
</x:beans>
