<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<x:beans xmlns:x="http://www.springframework.org/schema/beans"
	xmlns="http://www.bocom.com/schema/jump-core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd       http://www.bocom.com/schema/jump-core http://www.bocom.com/schema/jump-core-1.0.xsd">

	<!--  查询记录结果-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.QryCusBaseMsgAction"
		id="gdeupsb.qryCusBaseMsgAction" />
		
	<!--  查询抹账结果-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.TallyCancelQryOnlineAction"
		id="gdeupsb.tallyCancelQryOnlineAction" />	
	<!-- 预付款 -->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.AdvanceTradeAction"
		id="gdeupsb.advanceTradeAction"/>
	<!-- 无操作Action -->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.NullAction"
		id="gdeupsb.nullAction"/>
	<action   class="com.bocom.bbip.gdeupsb.action.efek.AdvanceTradeTimeAction"
		id="gdeupsb.advanceTradeTimeAction"/>	
		<action   class="com.bocom.bbip.gdeupsb.action.efek.AdvanceTradeStrCastDateAction"
		id="gdeupsb.advanceTradeStrCastDateAction"/>			
	<!--  交费记录查询-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.QryCusPayRecordAction"
		id="gdeupsb.qryCusPayRecordAction" />

	<!--  银行查询客户信息-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.QryCusMsgAction"
		id="gdeupsb.qryCusMsgAction" />	
		
		<!--  协议新增修改注销-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.CusAgentServiceAction"
		id="gdeupsb.cusAgentServiceAction" />	
		
	<!--  供电批量代扣取消交易-->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.batch.CancelBatchAction"
		id="gdeupsb.CancelBatchAction" />		
		
	<!--  异步调用 对总账-->
	<action   class="com.bocom.bbip.gdeupsb.strategy.efek.checkThdAcct.GdcheckThdSumAcctAction"
		id="gdeupsb.checkThdSumAcctAction" />	
		
	<action   class="com.bocom.bbip.gdeupsb.action.efek.AgentFileToThdAction"
		id="gdeupsb.AgentFileToThdAction" />					
		<!--  异步调用 对明细-->
	<action   class="com.bocom.bbip.gdeupsb.strategy.efek.checkThdAcct.GdcheckThdDetlAcctAction"
		id="gdeupsb.checkThdDetlAcctAction" />
		
	<!--  协议新增-->
	<action   class="com.bocom.bbip.gdeupsb.strategy.efek.agent.InsertCusAgentServiceAction"
		id="eups.commInsertCusAgentActionELEC00" />		
	<!--  协议修改-->
	<action   class="com.bocom.bbip.gdeupsb.strategy.efek.agent.UpdateCusAgentServiceAction"
		id="eups.commUpdateCusAgentActionELEC00" />		
	<!--  协议删除-->
	<action   class="com.bocom.bbip.gdeupsb.strategy.efek.agent.DeleteCusAgentServiceAction"
		id="eups.commDelCusAgentActionELEC00" />		

	<!-- 签到-->		
	<action   class="com.bocom.bbip.gdeupsb.action.efek.EfekStartAction"
		id="gdeupsb.efekStartAction" />		
	<!--  签退-->	
	<action   class="com.bocom.bbip.gdeupsb.action.efek.EfekEndAction"
		id="gdeupsb.efekEndAction" />		
		<!-- 电网清单报表 -->
	<action   class="com.bocom.bbip.gdeupsb.action.efek.batch.PrintEfekBatchAction"
		id="gdeupsb.printEfekBatchAction" />					
</x:beans>