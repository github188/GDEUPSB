<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/xml" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/xml xml.xsd"
    encoding="UTF-8"  trace="true" name="root">   
    <record name="pkgH">
        <string name="type"/>
        <string name="action" />
        <string name="version"/>
        <string name="dealer_id"/>
        <string name="terminal_id"/>
        <string name="mobile"/>
        <string name="phone"/>
        <string name="sent_time"/>
    </record>
        <select name="body" key="MsgType" default="N">
            <!-- 第三方请求的用 decodePrefix 向第三方请求的用 encodePrefix -->
            <include name="N" keys="action" encodePrefix="f"/>
        </select>
</message>