<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/xml" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/xml xml.xsd"
    encoding="UTF-8"  trace="true" name="pkg">
    <record name="pkgH">
        <string name="type" default="3"/>
        <string name="action"/>
        <string name="version"/>
        <string name="dealer_id" default="141"/>
        <string name="terminal_id" default="0"/>
        <string name="mobile" default="0"/>
        <string name="phone" default="0"/>
        <string name="sent_time" default="2015-01-14 09:25:07"/>
    </record>
        <select name="body" key="MsgType" default="N">
            <include name="N" keys="action" decodePrefix="p"/>
        </select>
</message>