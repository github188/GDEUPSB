<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
        filter="both,WEB-INF/classes/config/stream/SGRT00/translateDict.txt"
         type="LLLL" encoding="UTF-8" trace="true">
    <segment name="agentResponsehead">
        <fixString name="TransCode" length="4" referred="true" mapping="true"/>
       <!--  <fixString name="TRADE_ID" length="4" referred="true" mapping="true"/>   //[JUMPCO7000] no_process_id -->
        <fixString name="TRAN_TIME" length="14" />
        <fixString name="BANK_ID" length="10" />
        <fixString name="DPT_ID" length="4" />
        <fixString name="TRADE_SEQ" length="30" />
        <fixString name="APP_TYPE" length="15" />

    </segment>
   <include name="body" keys="TransCode" encodePrefix="p" />
</message>


