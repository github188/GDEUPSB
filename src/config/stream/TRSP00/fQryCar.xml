<?xml version="1.0" encoding="UTF-8"?>
<segment xmlns="http://www.bocom.com/jump/transformer/packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
	name="body" >
	 <varString type="D|"  name="carNo"  expression="exp:@com.bocom.bbip.gdeupsb.utils.GdExpCommonUtils@AddChar(this.carNo, 20, ' ','0')"/> <!-- 车牌号码 -->
	  <varString type="D|"  name="carTyp"  expression="exp:@com.bocom.bbip.gdeupsb.utils.GdExpCommonUtils@AddChar(this.carTyp, 3, ' ','0')"/> <!-- 车牌类别-->
	   <varString type="D|"  name="payMon" expression="exp:@com.bocom.bbip.gdeupsb.utils.GdExpCommonUtils@AddChar(this.payMon, 2, ' ','0')" /> <!-- 缴费月数 -->
	 <varString type="D|"  name="tactDt"  /> <!-- 交易日期 -->
	 <varString type="D|" name="tlogNo"  /> <!-- 流水号-->
</segment>