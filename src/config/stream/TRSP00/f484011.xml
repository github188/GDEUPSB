<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
	encoding="GBK" trace="true">
	<segment name="head" encoding="GBK">
		<varString type="D|" name="bk" default="100" /> <!-- 银行代码 -->
		<varString type="D|" name="tlr" /> <!-- 机构代码 -->
		<varString type="D|" name="tlrTmlId" /> <!-- 操作员号 -->
		<varString type="D|" name="sup1Id" default=" " /> <!-- 授权号 -->
		<varString type="D|" name="carNo" /> <!-- 车牌号码 -->
		<varString type="D|" name="carTyp" /> <!-- 车牌类别 -->
		<varString type="D|" name="payMon" /> <!-- 缴费月数 -->
		<varString type="D|" name="tactDt" /> <!-- 交易日期 -->
		<varString type="D|" name="tlogNo" /> <!-- 流水号 -->
	</segment>
</message>