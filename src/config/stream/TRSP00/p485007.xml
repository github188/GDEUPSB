<?xml version="1.0" encoding="UTF-8"?>
<segment xmlns="http://www.bocom.com/jump/transformer/packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/packet packet.xsd"
	name="ChgIPCResponse" >
	<fixString length="6" name="trspCd" /> <!-- 返回码-->                                                                              
	<fixString length="20" name="plateNo" /> <!-- 车牌号码-->
	<fixString length="3" name="plateType" /> <!-- 车牌类别-->
	<fixString length="2" name="monthCount" /> <!-- 缴费月数-->
	<fixString length="8" name="begDat" /> <!-- 有效起始日期-->
	<fixString length="8" name="endDat" /> <!-- 有效结束日期-->
	<fixString length="20" name="carName" /> <!-- 车型说明-->
	<fixString length="10" name="carDzs" /> <!-- 吨数或座数-->
	<fixString length="6" name="cntStd" /> <!-- 计费标准-->
	<fixString length="18" name="corpus" /> <!-- 本金-->
	<fixString length="18" name="delayFee" /> <!-- 滞纳金额-->
	<fixString length="18" name="oweAmount" /> <!-- 总金额 -->
	<fixString length="10" name="CLGS" /> <!-- 车辆归属-->
	<fixString length="10" name="YYBZ" /> <!-- 营运标志-->
	<fixString length="60" name="custName" /> <!-- 车主名称-->
	<fixString length="18" name="feeStd" /> <!-- 标准年收费额-->
	
</segment>