<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
						http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">


	<!-- 欠费查询-->
	<bean id="eups.qryDealStrategyActionELEC01"
		class="com.bocom.bbip.gdeups.strategy.elcgd.QueryElecUserStrategyAction">
	</bean>


	<!-- 初始化策略 -->
	<bean id="eups.prepareCheckDealStrategyActionELC01"
		class="com.bocom.bbip.gdeups.strategy.elcgd.PreInitElecStrategyAction">
	</bean>
	<!-- 记账前策略 -->
	<bean id="eups.preparePayStrategyActionELEC01"
		class="com.bocom.bbip.gdeups.strategy.elcgd.PrePayElecStrategyAction">
	</bean>
	<!-- 调用第三方前策略 -->
	<bean id="eups.prePayFeeStrategyActionELEC01"
		class="com.bocom.bbip.gdeups.strategy.elcgd.PreThdElecStrategyAction">
	</bean>


	<!-- 抹帐调用第三方前策略-->
	<action class="com.bocom.bbip.gdeups.strategy.elcgd.PreCnlElecStrategyAction"
		id="eups.preCancelStrategyActionELEC01" />



</beans>