<?xml version="1.0" encoding="UTF-8"?>
<message xmlns="http://www.bocom.com/jump/transformer/8583packet"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.bocom.com/jump/transformer/8583packet iso8583.xsd"
	encoding="GBK" trace="true">
	
 <!-- 主账号,磁条卡的第二磁道或第三磁道上获取的账号，或在交易时输入的主账户号，或读IC卡获得的卡号 -->
	<varString name="cusAcEx" index="2" type="C"  format="bcd" bcdLeftJustified="true" dynBitSet="true" /> 
	<!-- 交易处理码 -->
	<fixString name="thdPayTyp" index="3" length="6"  format="bcd"  />
	<!-- 交易金额  -->
	<fixString name="amount" index="4" length="12"  format="bcd"  dynBitSet="true"/>
	<!-- 交易传输时间 -->
	<fixString name="txnDateTime" index="7"  length="10" format="bcd" dynBitSet="true"/>
 	<!-- 受卡方系统跟踪号 -->
	<fixString name="transJournal" index="11" length="12" format="bcd" dynBitSet="true"/>
	<!-- 受卡方所在地时间 -->
	<fixString name="bnkTxnTime" index="12"  length="6" format="bcd" dynBitSet="true"/>
	<!-- 受卡方所在地日期 -->
	<fixString name="bnkTxnDate" index="13"  length="4" format="bcd" dynBitSet="true"/>
	<!-- 清算日期. -->
	<fixString name="pwrtxnDate" index="15"  length="8" format="bcd" dynBitSet="true"/>
	<!-- 商户类型 -->
	<fixString name="pwrFeePnt" index="18" length="4" filling="0x00" dynBitSet="true"/>    <!--配营部类型-->
		<!--交易手续费-->
	<fixString name="pwrFee" index="28" length="12" dynBitSet="true"/>   
	<!-- 受理方机构标识码 -->
	<varString name="eleBkNo" index="32" type="C"  format="bcd" dynBitSet="true"/>
	<!-- 供电公司系统参考号-->
	<fixString name="eleThdSqn" index="37" length="12"  format="bcd"   dynBitSet="true"/>
	<!--应答码.-->
	<fixString name="thdRspCde" index="39" length="2" dynBitSet="true"/>
	<!-- 受理方终端标识码-->
	<fixString name="tTrmId" index="41" length="8"     dynBitSet="true"/>
	<!-- 受理方标识码-->
	<fixString name="thdCusNo" index="42" length="15"    dynBitSet="true"/>
	<!-- 附加数据－私有  -->
    <varString name="remarkData" index="48" type="CC" dynBitSet="true"/>
	<!-- 交易货币代码,人民币的货币代码为156.-->
	<fixString name="CCY" index="49" length="3" filling="0x00"  dynBitSet="true"/>
    <!-- 接收机构标识代码-->
	<varString name="thdRgnNo" index="100" type="C"  format="bcd"  dynBitSet="true"/>
    <!-- F104 --> 
    <fixString name="traTyp" index="104" length="2" filling="0x00"  dynBitSet="true"/>
	<!-- MAC. -->
 	<fixString name="msgIdfCde" index="128" length="16" format="bcd" dynBitSet="true"/>
 </message>