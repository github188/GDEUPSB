<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context" xmlns:tx="http://www.springframework.org/schema/tx"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
						http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-3.0.xsd">


	<!-- 欠费查询-->
	<bean id="eups.qryDealStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.QueryElecUserStrategyAction">
	</bean>


	<!-- 初始化策略 -->
	<bean id="eups.prepareCheckDealStrategyActionELC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PreInitElecStrategyAction">
	</bean>
	<!-- 记账前策略 -->
	<bean id="eups.preparePayStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PrePayElecStrategyAction">
	</bean>
	<!-- 调用第三方前策略 -->
	<bean id="eups.prePayFeeStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PreThdElecStrategyAction">
	</bean>

	<!-- 调用第三方后策略 -->
	<bean id="eups.aftPayFeeStrategyAction"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.AftThdElecStrategyAction">
	</bean>

	<!-- 调用第三方前策略 -->
	<bean id="eups.preCancelStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PreCnlElecStrategyAction">
	</bean>


	
	<!-- 第三方发起的划扣，初始策略-->
	<bean id="eups.prepareCheckDealStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PreEleCheckDealStrategyAction">
	</bean>
	
	<bean id="eups.aftPayToBankStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.AftPayDealStrategyAction">
	</bean>
	
		<!-- 第三方发起的划扣撤销，初始策略-->
	<bean id="eups.preCclToBankStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.PreCnlBnkSglDealStrategyAction">
	</bean>
	
		<!-- 第三方发起的划扣撤销，返回策略-->
	<bean id="eups.aftCclToBankStrategyActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.AftCnlBnkSglDealStrategyAction">
	</bean>
	
	
	<!-- BatchAcpElecDealServiceAction -->
	
			<!-- 第三方发起的划扣撤销，返回策略-->
	<bean id="eups.fileBatchPayCreateDataServiceActionELEC01"
		class="com.bocom.bbip.gdeupsb.strategy.elcgd.BatchAcpElecDealServiceAction">
	</bean>
	
</beans>